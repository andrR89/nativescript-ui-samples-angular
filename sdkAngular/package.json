{
  "name": "nativescript-telerik-ui-sdk-angular-examples",
  "version": "1.6.1",
  "description": "Nativescript project that shows the NativeScript UI with Angular2",
  "license": "SEE LICENSE IN <your-license-filename>",
  "readme": "NativeScript Application",
  "repository": {
    "type": "git",
    "url": "git@github.com/telerik/nativescript-ui-samples-angular.git"
  },
  "nativescript": {
    "id": "org.nativescript.sdkAngular",
    "tns-android": {
      "version": "2.5.0"
    },
    "tns-ios": {
      "version": "2.5.0"
    }
  },
  "dependencies": {
    "@angular/common": "~2.4.5",
    "@angular/compiler": "~2.4.5",
    "@angular/core": "~2.4.5",
    "@angular/forms": "~2.4.5",
    "@angular/http": "~2.4.5",
    "@angular/platform-browser": "~2.4.5",
    "@angular/platform-browser-dynamic": "~2.4.5",
    "@angular/router": "~3.4.5",
    "nativescript-angular": "1.4.1",
    "nativescript-fresco": "^1.0.11",
    "nativescript-intl": "^0.0.4",
    "nativescript-telerik-ui-pro": "1.6.1",
    "nativescript-unit-test-runner": "^0.3.2",
    "rxjs": "~5.0.1",
    "tns-core-modules": "2.5.1"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~2.4.5",
    "@ngtools/webpack": "1.2.10",
    "babel-traverse": "6.11.4",
    "babel-types": "6.11.1",
    "babylon": "6.8.4",
    "codelyzer": "~2.0.0-beta.3",
    "copy-webpack-plugin": "~3.0.1",
    "css-loader": "~0.26.0",
    "extract-text-webpack-plugin": "~2.0.0-beta.5",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "htmlparser2": "^3.9.2",
    "lazy": "1.0.11",
    "markdown-snippet-injector": "^0.2.0",
    "nativescript-css-loader": "~0.26.0",
    "nativescript-dev-android-snapshot": "~0.0.6",
    "nativescript-dev-typescript": "^0.3.2",
    "nativescript-dev-webpack": "^0.3.4",
    "opener": "^1.4.1",
    "raw-loader": "~0.5.1",
    "resolve-url-loader": "~1.6.0",
    "rimraf": "^2.5.3",
    "tar.gz": "^1.0.5",
    "tns-platform-declarations": "^2.4.0",
    "tslint": "~4.0.2",
    "typescript": "~2.1.0",
    "webpack": "2.2.0",
    "webpack-sources": "~0.1.3",
    "zone.js": "~0.7.2"
  },
  "scripts": {
    "pretsc": "npm install",
    "tsc": "tsc",
    "pretslint": "npm run tsc",
    "tslint": "tslint -C tsconfig.json --project tslint.json",
    "prebuild": "npm run tslint",
    "build": "node scripts/build.js",
    "postbuild": "npm run inject && npm run archive",
    "inject": "mdinject --root=app --docsroot=dist/sdk-examples --sourceext=\".ts|.css|.html\" --snippettitles=\"TypeScript|CSS|HTML\"",
    "archive": "node scripts/archive.js",
    "prepush-to-github": "npm run build",
    "push-to-github": "(cd dist/sdk-examples && rimraf .git && git init && git add -A && git commit -m \"nativescript-sdk-examples build preview\" && git remote add origin git@github.com:NativeScript/nativescript-sdk-examples-preview.git && git push -u --force origin master)",
    "preshow-preview": "npm run push-to-github",
    "show-preview": "opener https://github.com/NativeScript/nativescript-sdk-examples-preview",
    "ns-bundle": "ns-bundle",
    "start-android-bundle": "npm run ns-bundle --android --start-app",
    "start-ios-bundle": "npm run ns-bundle --ios --start-app",
    "build-android-bundle": "npm run ns-bundle --android --build-app",
    "build-ios-bundle": "npm run ns-bundle --ios --build-app"
  }
}
